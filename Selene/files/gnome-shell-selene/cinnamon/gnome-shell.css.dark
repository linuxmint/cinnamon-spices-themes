/*GNOME Shell Pantheon theme modded by tista*/

stage {
    font-family: ubuntu, cantarell, droid sans, sans-serif;
}

.shell-link {
    color: #fff;
    text-decoration: underline;
}

.shell-link:hover {
    color: #6eabec;
}

.label-shadow {
    color: rgba(255,255,255,0);
}

.label-real-shadow {
    background-gradient-direction: horizontal;
    background-gradient-start: rgba(0, 0, 0, 0);
    background-gradient-end: rgba(0, 0, 0, 255);
    width: 10px;
}

StScrollBar
{
  padding: 3px;
  padding-left: 6px;
}

StScrollView StScrollBar
{
  min-width: 4px;
  min-height: 16px;
}


StScrollBar StBin#trough {
/*    background-color: #DEDEDE; */
    border-radius: 2px;
}

StScrollBar StButton#vhandle
{
    border-radius: 2px;
    background-color: #6e6e6e;
}

StScrollBar StButton#hhandle
{
    border-radius: 2px;
    background-color: #6e6e6e;
}

StScrollBar StButton#hhandle:hover,
StScrollBar StButton#vhandle:hover
{
    background-color: #3880ca;
}

StTooltip StLabel {
    background-color: rgba(0,0,0,0.7);
    border: 1px solid rgba(255,255,255,0.5);
    border-radius: 4px;
    padding: 4px;
    color: #fff;
    font-size: 0.8em;
    font-weight: normal;
    text-align: center;
}

/* PopupMenu */


.popup-menu-boxpointer {
    -arrow-border-radius: 0px;
    -arrow-background-color: rgba(0,0,0,0);
    -arrow-border-width: 0px;
    -arrow-border-color: #a5a5a5;
    -arrow-base: 0px;
    -arrow-rise: 0px;
    border-color: rgba(0,0,0,0.4);
    border-radius: 0px;
    background-color: rgba(38,38,38,1);
    border-width: 1px;
    box-shadow: 0px 0px 6px 4px rgba(0,0,0,0.7);
}

.popup-menu {
    color: rgba(255,255,255,1);
    min-width: 200px;
}

.popup-sub-menu {
    background-color: rgba(38,38,38,1);
}

/* The remaining popup-menu sizing is all done in ems, so that if you
 * override .popup-menu.font-size, everything else will scale with it.
 */
.popup-menu-content {
    padding: 0.4em 0em;
}

.popup-menu-item {
    padding: 0.4em 1.75em;
    spacing: 0.4em;
}

.popup-menu-item:active {
    background-gradient-direction: vertical;
    background-gradient-start: #6eabec;
    background-gradient-end: #3880ca;
    color: #fff;
    text-shadow: rgba(38,38,38,0.75) 0px -1px 0px;
    box-shadow: inset 0px 0px 1px 1px rgba(255,255,255,0.1);
}

.popup-image-menu-item {
}

.popup-alternating-menu-item:alternate {
    font-weight: bold;
}

.popup-separator-menu-item {
    background-color: rgba(0,0,0,0.3);
    border: 1px solid rgba(120,120,120,0.2);
    border-left: 0px;
    border-right: 0px;
    border-top: 0px;
    height: 0.1em;
}

.popup-slider-menu-item {
    height: 1em;
    min-width: 15em;
    -slider-height: 0.4em;
    -slider-background-color: #cccccc;
    -slider-border-color: #5f5f5f;
    -slider-active-background-color: #cccccc;
    -slider-active-border-color: #5f5f5f;
    -slider-border-width: 1px;
    -slider-handle-radius: 0.5em;
}

.popup-device-menu-item {
    spacing: .5em;
}

.popup-menu-icon {
    icon-size: 1.14em;
}

/* Switches (to be used in menus) */
.toggle-switch {
    width: 5em;
    height: 2.5em;
}

.toggle-switch-us {
    background-image: url("toggle-off-us.svg");
}
.toggle-switch-us:checked {
    background-image: url("toggle-on-us.svg");
}

.toggle-switch-intl {
    background-image: url("toggle-off-intl.svg");
}
.toggle-switch-intl:checked {
    background-image: url("toggle-on-intl.svg");
}

/* Panel */

#panel {
    background-gradient-direction: vertical;
    background-gradient-start: #3a3a3a;
    background-gradient-end: #1e1e1e;
    border: 1px solid rgba(0,0,0,0.25);
    border-top: 0px;
    border-left: 0px;
    border-right: 0px;
    padding-top: 0px;
    font-size: 10pt;
    font-weight: bold;
    height: 24px;
    box-shadow: 0px 0px 6px 4px rgba(0,0,0,1);
}

#panelCenter {
    spacing: 4px;
    padding-top: 2px;
}

#panelLeft {
    spacing: 8px;
    padding-right: 4px;
    padding-left: 4px;    
    padding-top: 1px;
}

#panelRight {
    padding-left: 4px;
    padding-top: 1px;
}

#appMenu {
    spacing: 6px;
    padding-top: 1px;
}

.panel-button #appMenuIcon {
    padding-top: 4px;
    app-icon-bottom-clip: 0px;
}

.panel-button:active #appMenuIcon,
.panel-button:checked #appMenuIcon,
.panel-button:focus #appMenuIcon {
    app-icon-bottom-clip: 0px;
}

.app-menu-icon {
    width: 24px;
    height: 24px;
}

.panel-button {
    -natural-hpadding: 6px;
    -minimum-hpadding: 4px;
    transition-duration: 100;
    color: #dedede;
    icon-shadow: rgba(0,0,0,0.75) 0px 1px 1px;
}

.panel-button:hover {
    color: #6eabec;
    text-shadow: rgba(0,0,0,0.75) 0px -1px 0px;
}

.panel-button:active,
.panel-button:checked,
.panel-button:focus {
    border: 0px solid rgba(86,147,201,0.8);
    background-color: none;
    border-bottom: 0px;
    border-top: 0px;
    color: #6eabec;
    text-shadow: rgba(0,0,0,0.75) 0px -1px 0px;
}

panel-status-button > .panel-button:active,
panel-status-button > .panel-button:checked,
panel-status-button > .panel-button:focus {
    color: #dedede;
    text-shadow: rgba(0,0,0,0.75) 0px -1px 0px;
    icon-shadow: rgba(0,0,0,0.75) 0px 1px 1px;
}

.panel-button:active > .system-status-icon,
.panel-button:checked > .system-status-icon,
.panel-button:focus > .system-status-icon {
    color: #dedede;
    text-shadow: rgba(0,0,0,0.75) 0px -1px 0px;    
    icon-shadow: rgba(0,0,0,0.75) 0px 1px 1px;
}

#panelActivities {
/*    color: #383838; */
/*    text-shadow: rgba(255,255,255,0.75) 0px 1px 1px; */
/*    border-radius-topleft: 0px; */
/*    border-radius-bottomleft: 0px; */
    
    color: rgba(0,0,0,0);
    height: 22px;
    width: 22px;
    background-image: url("logo.svg");
    border-radius-topleft: 0px;
    border-radius-bottomleft: 0px;    
}


#panelStatus {
    border-radius-topright: 0px;
}

.panel-menu {
    -boxpointer-gap: 4px
}

#panelUserMenu {
    spacing: 4px;
}

.status-chooser {
    spacing: .4em;
}

.status-chooser .popup-menu-item,
.status-chooser-combo .popup-menu-item {
    padding: .4em;
}

.status-chooser-user-icon {
    border: 0px solid #8b8b8b;
    border-radius: 4px;
    width: 48pt;
    height: 48pt;
}

.status-chooser-user-icon:hover {
    background-color: #4a90d9;
    color: #fff;
    box-shadow: inset 1px 0px 0px 0px rgba(255,255,255,0.1);
}

.status-chooser-user-name {
    font-weight: bold;
    font-size: 10pt;
    min-width: 76pt;
}

.status-chooser-combo {
    border: 1px solid transparent;
}

.status-chooser-combo.popup-combo-menu {
    background-color: rgba(38,38,38,1);
    border-radius: 4px;
    border: 0px solid #5f5f5f;
    color: #ffffff;
    font-size: 10pt;
    box-shadow: 0px 0px 6px 4px rgba(0,0,0,0.7);
}

.status-chooser-status-item,
.status-chooser-combo > .popup-combobox-item {
    spacing: .4em;
    font-size: 10pt;
}

.system-status-icon {
    icon-size: 1.27em;
}

/* Overview */

#overview {
    spacing: 12px;
/*    background-gradient-direction: vertical; */
/*    background-gradient-start: #e4e4e4; */
/*    background-gradient-end: #a4a4a4; */
/*    background-color: #a1a1a1; */
    background-image: url("overview.jpg");
}

.workspaces-view {
    spacing: 64px;
}

.workspace-controls {
    visible-width: 12px; /* Amount visible before hovering */
}

.workspace-thumbnails-background {
    background-color: #383838;
    box-shadow: inset 0px 0px 6px 2px rgba(0,0,0,0.5);
    border-right: 0px;
    border-radius: 8px 0px 0px 8px;
    padding: 12px;
}

.workspace-thumbnails-background:rtl {
    border-right: 1px;
    border-left: 0px;
    border-radius: 0px 9px 9px 0px;
}

.workspace-thumbnails {
    spacing: 12px;
}

.workspace-thumbnail-indicator {
    outline: 3px solid #6eabec;
}

.window-caption {
    padding: 4px;
    color: #383838;
    text-shadow: rgba(255,255,255,0.75) 0px 1px 1px;    
    font-size: 10pt;
    font-weight: bold;
    padding: 2px 8px;
    -shell-caption-spacing: 8px;
}

.window-close {
    background-image: url("close-window.svg");
    height: 24px;
    width: 24px;
    -st-background-image-shadow: -2px 2px 6px rgba(0,0,0,0.5);
    -shell-close-overlap: 16px;
}

/* Dash */

#dash {
/*    spacing: 22px; */
    border-image:url("dash.png");
    border-right: 0px ;
    border-bottom: 0px ;
    border-top: 0px ;
    padding-bottom: 4px;
    padding-top: 0px;
}

#dash:rtl {
    border-left: 1px;
    border-right: 0px;
    border-radius: 9px 0px 0px 9px;
}

#dash:empty {
    height: 100px;
    width: 30px;
}

.dash-placeholder {
    background-image: url("dash-placeholder.svg");
    height: 27px;
}

#viewSelector {
    spacing: 32px;
}

#viewSelectorTabBar {
    padding: 12px 0px 12px 8px;
}

#searchArea {
    padding: 12px 12px 12px 8px;
}

#searchEntry {
    padding: 4px 10px;
    background-color: #dedede;
    border: 1px solid rgba(0,0,0,0.3);
    border-radius: 4px;
    box-shadow: inset 0px 2px 8px 0px rgba(0,0,0,0.25);
    selected-color: #fff;
    color: #383838;
    caret-color: #383838;
    caret-size: 1px;
    height: 16px;
    width: 250px;
    font-weight: normal;
}

#searchEntry:focus {
    background-color: #dedede;
    border: 1px solid rgba(0,0,0,0.3);
    border-radius: 4px;
    box-shadow: inset 0px 2px 8px 0px rgba(0,0,0,0.15);
    color: #383838;
    caret-color: rgb(128, 128, 128);
    caret-size: 1px;
    height: 16px;
    width: 250px;
    font-weight: bold;    
}

#searchEntry:hover {
    background-color: #dedede;
    border: 1px solid rgba(0,0,0,0.3);
    border-radius: 4px;
    box-shadow: inset 0px 2px 8px 0px rgba(0,0,0,0.25);
    color: #383838;
    caret-color: rgb(128, 128, 128);
    caret-size: 1px;
    height: 16px;
    width: 250px;
    font-weight: bold;    
}

.search-entry-icon {
    icon-size: 1em;
    color: #383838;
}

