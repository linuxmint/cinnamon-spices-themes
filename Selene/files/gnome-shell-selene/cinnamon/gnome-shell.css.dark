    text-shadow: rgba(255,255,255,0.75) 0px 1px 1px;
}

.app-filter:selected {
    background-color: rgba(0,0,0,0);
    color: #4a90d9;    
    text-shadow: rgba(255,255,255,0.75) 0px 1px 1px;
}

.dash-item-container > .app-well-app {
    padding: 4px 14px;
}

.remove-favorite-icon {
    color: #a0a0a0;
}

.remove-favorite-icon:hover {
    color: white;
    icon-shadow: black 0px 2px 2px;
}

.app-well-app > .overview-icon,
.remove-favorite > .overview-icon,
.search-result-content > .overview-icon {
    color: #fff;
    padding: 2px;
    text-align: center;
    text-shadow: rgba(0,0,0,0.75) 0px -1px 0px;
}

.app-well-app .running > .overview-icon {
    background-gradient-direction: vertical;
    background-gradient-start: #6eabec;
    background-gradient-end: #3880ca;
