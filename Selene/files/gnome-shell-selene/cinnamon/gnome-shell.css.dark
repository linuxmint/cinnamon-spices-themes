/* ClutterGroup */

#clutterGroup {
    border-width: 6px;
    padding: 6px;
    box-shadow: 0px 0px 6px 2px rgba(0,0,0,0.5);
}

/* Apps */

.icon-grid {
    spacing: 36px;
    padding: 20px;
    -shell-grid-item-size: 64px;
    background-color: #383838;
    border: 1px solid rgba(255,255,255,0.1);
    border-radius: 8px;
    box-shadow: inset 0px 0px 6px 2px rgba(0,0,0,0.5);
}

.all-app {
    spacing: 20px;
    padding: 0px;
}

.all-app .icon-grid {
    -shell-grid-item-size: 84px;
}


.contact-grid {
    spacing: 46px;
    -shell-grid-item-size: 256px;
    padding: 22px;
    padding-bottom: 0px;
    background-color: #383838;
    border: 1px solid rgba(255,255,255,0.1);
    border-radius: 8px;
    box-shadow: inset 0px 0px 6px 2px rgba(0,0,0,0.5);
}

.all-app .overview-icon {
    icon-size: 56px;
}

.app-filter {
